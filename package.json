{
  "name": "kiosk",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/apps/*",
    "packages/shared/*",
    "packages/config/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\""
  },
  "devDependencies": {
    "@kiosk/eslint-config": "*",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "clsx": "^1.1.1",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.5.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
